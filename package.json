{
  "name": "vue-is-defined",
  "version": "1.0.0",
  "description": "Vue 3 type guard checking if ref value is not null/undefined",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "logo.svg",
    "banner.svg"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue-3",
    "composable",
    "composition-api",
    "vueuse",
    "utility",
    "helper",
    "utils",
    "defined",
    "nullish",
    "type-guard",
    "typescript",
    "check",
    "null",
    "undefined",
    "isDefined"
  ],
  "author": "VueFrag",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuefrag/vue-is-defined.git"
  },
  "bugs": {
    "url": "https://github.com/vuefrag/vue-is-defined/issues"
  },
  "homepage": "https://github.com/vuefrag/vue-is-defined#readme",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "unbuild": "^2.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.4.0"
  }
}